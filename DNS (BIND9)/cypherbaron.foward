@       IN      SOA     ns1.cypherbaron.local. root.cypherbaron.local. (
                        2025122801      ; Serial
                        86400           ; Refresh
                        1200            ; Retry
                        2419200         ; Expire
                        1200 )          ; Negative Cache TTL

